.select{
  position: relative;
  z-index: 2;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.data{
  box-sizing: border-box;
  height: 65rpx;
  width: 100%;
  border: 1rpx solid #9c9c9c;
  border-radius: 20rpx;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0 50rpx 0 20rpx;
  font-size: 25rpx;
}
.data view{
  white-space: nowrap;
  overflow-x: auto;
}
.data image{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right: 20rpx;
}
.list{
  width: 95%;
  height: 200rpx;
  border-radius: 10rpx;
  background: #cbcbcb;
  position: relative;
  left: 0;
  top: 10rpx;
  z-index: 2;
  font-size: 25rpx;
  color: white
}
.list view{
  box-sizing: border-box;
  height: 65rpx;
  border-bottom: 0.1rpx solid #9c9c9c;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  white-space: nowrap;
  overflow-x: auto;
}