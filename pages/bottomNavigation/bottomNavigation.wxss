.bottom-navigation{
  box-sizing: border-box;
  width: 100%;
  height: 111rpx;
  border-top: 0.5rpx solid #cbcbcb;
  background: white;
  position: absolute;
  bottom: 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 23rpx;
  color: #cbcbcb;
}
.bottom-navigation view{
  display: flex;
  flex-direction: column;
  align-items: center;
}
image{
  width: 48rpx;
  height: 48rpx;
}
.bottom-add{
  width: 96rpx;
  height: 96rpx;
  align-self: flex-start;
}
.bottom-add:active{
  transform: scale(0.9)
}